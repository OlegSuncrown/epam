],"import { Switch, Route, useRouteMatch, Link } from \"react-router-dom\";\nimport { Row, Col, Container } from \"react-bootstrap\";\nimport { ProfilePage, UserGoalsPage, SeasonalGoals, BagesPage, Goal } from \"./pages\";\nimport { SideBar } from \"./components\";\nimport SingleGoal from \"./pages/goals/SingleGoal\";\n\nconst Dashboard = () => {\n  let { path } = useRouteMatch();\n  return (\n    <>\n      <Container className=\"py-5\">\n        <Row>\n          <Col xs={12} lg={3} className=\"d-none d-lg-block\">\n            <SideBar />\n          </Col>\n          <Col className=\"mx-auto\">\n            <Switch>\n              <Route exact path={path} component={ProfilePage} />\n              <Route exact path={`${path}/goals/:id`} component={Goal} />\n              <Route exact path={`${path}/add-goal/`} component={SingleGoal} />\n              <Route path={`${path}/user-goals`} component={UserGoalsPage} />\n              <Route\n                path={`${path}/seasonal-goals`}\n                component={SeasonalGoals}\n              />\n              <Route path={`${path}/user-bages`} component={BagesPage} />\n            </Switch>\n          </Col>\n        </Row>\n      </Container>\n    </>\n  );\n};\n\nexport default Dashboard;\n","/Users/tetianagurova/workspace/projects/react/epam/src/pages/index.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/layouts/index.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/components/index.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/pages/login/LoginPage.js",["146"],"import React, { useContext, useState, useEffect } from \"react\";\nimport { Form, Button, Col, Container } from \"react-bootstrap\";\nimport { AuthContext } from \"../../context/auth/AuthContext\";\nimport { ToastContainer, toast } from \"react-toastify\";\n\nconst LoginPage = ({ history }) => {\n  const { loginUser, isAuthenticated } = useContext(AuthContext);\n\n  const [userLogin, setUserLogin] = useState({\n    email: \"\",\n    password: \"\",\n  });\n\n  const [loading, setLoading] = useState(false);\n\n  // Get value from the form\n  const onChange = (e) =>\n    setUserLogin({ ...userLogin, [e.target.name]: e.target.value });\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n\n    try {\n      setLoading(true);\n      return await loginUser(userLogin);\n    } catch (err) {\n      toast.error(err.message, {\n        position: \"top-right\",\n        autoClose: 3000,\n        hideProgressBar: false,\n        closeOnClick: true,\n        pauseOnHover: false,\n        draggable: true,\n        progress: undefined,\n      });\n    }\n\n    setLoading(false);\n  };\n\n  useEffect(() => {\n    if (isAuthenticated) {\n      history.push(\"/dashboard\");\n    }\n  }, [isAuthenticated]);\n\n  return (\n    <Container>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={3000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick\n        rtl={false}\n        pauseOnFocusLoss\n        draggable\n        pauseOnHover\n      />\n      <div className=\"card m-3 p-3\">\n        <h2 className=\"text-center mb-3\">\n          <span className=\"text-primary\">Log</span> in\n        </h2>\n        <Form onSubmit={handleSubmit} className=\"h-100\">\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Email address</Form.Label>\n              <Form.Control\n                required\n                type=\"email\"\n                placeholder=\"Enter email\"\n                name=\"email\"\n                onChange={onChange}\n              />\n              <Form.Control.Feedback>Looks good!</Form.Control.Feedback>\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"formBasicPassword\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                placeholder=\"Password\"\n                name=\"password\"\n                required\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center mt-3\">\n            <Button\n              to=\"/addGoal\"\n              variant=\"primary\"\n              type=\"submit\"\n              className=\"px-4\"\n              disabled={loading}\n            >\n              Log in\n            </Button>\n          </Form.Row>\n        </Form>\n      </div>\n    </Container>\n  );\n};\n\nexport default LoginPage;\n","/Users/tetianagurova/workspace/projects/react/epam/src/pages/bages/BagesPage.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/pages/register/RegisterPage.js",["147"],"import React, { useContext, useState, useEffect } from \"react\";\nimport { Form, Button, Col, Container } from \"react-bootstrap\";\nimport { AuthContext } from \"../../context/auth/AuthContext\";\nimport { ToastContainer, toast } from \"react-toastify\";\nconst Register = ({ history }) => {\n  const { registerUser, isAuthenticated } = useContext(AuthContext);\n\n  const [loading, setLoading] = useState(false);\n  const [userRegister, setUserRegister] = useState({\n    firstName: \"\",\n    lastName: \"\",\n    email: \"\",\n    password: \"\",\n  });\n\n  //Redirect user if isAuthenticated\n  useEffect(() => {\n    if (isAuthenticated) {\n      history.push(\"/dashboard\");\n    }\n  }, [isAuthenticated]);\n\n  // Get value from the form\n  const onChange = (e) =>\n    setUserRegister({ ...userRegister, [e.target.name]: e.target.value });\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n\n    try {\n      setLoading(true);\n      return await registerUser(userRegister);\n    } catch (err) {\n      toast.error(err.message, {\n        position: \"top-right\",\n        autoClose: 3000,\n        hideProgressBar: false,\n        closeOnClick: true,\n        pauseOnHover: false,\n        draggable: true,\n        progress: undefined,\n      });\n    }\n\n    setLoading(false);\n  };\n  return (\n    <Container>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={3000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick\n        rtl={false}\n        pauseOnFocusLoss\n        draggable\n        pauseOnHover\n      />\n      <div className=\"card m-3 p-3\">\n        <h2 className=\"text-center mb-3\">\n          <span className=\"text-primary\">Register</span> Account\n        </h2>\n        <Form onSubmit={handleSubmit}>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>First name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"First name\"\n                name=\"firstName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Last name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"Last name\"\n                name=\"lastName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Email address</Form.Label>\n              <Form.Control\n                required\n                type=\"email\"\n                placeholder=\"Enter email\"\n                name=\"email\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"formBasicPassword\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                minlength=\"6\"\n                placeholder=\"Password\"\n                required\n                name=\"password\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center mt-3\">\n            <Button\n              disabled={loading}\n              to=\"/addGoal\"\n              variant=\"primary\"\n              type=\"submit\"\n              className=\"px-4\"\n            >\n              Register\n            </Button>\n          </Form.Row>\n        </Form>\n      </div>\n    </Container>\n  );\n};\n\nexport default Register;\n","/Users/tetianagurova/workspace/projects/react/epam/src/pages/profile/ProfilePage.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/pages/seasonalGoals/SeasonalGoalsPage.js",["148","149","150"],"import React, { useState, useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { Container, Col, Row } from \"react-bootstrap\";\nimport GoalItem from \"../userGoals/GoalItem\";\n\nconst SeasonalGoals = (props) => {\n  // const goalsList = useContext(GoalsContext);\n\n  return (\n    <Container fluid>\n      <Row>\n        <Col xs={6} md={4}>\n          Title\n        </Col>\n        <Col className=\" col-sm-3\">Start date</Col>\n        <Col className=\"d-none d-sm-block col-sm-3\">End Date</Col>\n        <Col className=\"d-none d-md-block\">Progress</Col>\n      </Row>\n\n      {/* {goalsList.map(item => {\n                 (<GoalItem \n          