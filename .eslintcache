lick: true,\n        pauseOnHover: false,\n        draggable: true,\n        progress: undefined,\n      });\n    }\n\n    setLoading(false);\n  };\n  return (\n    <Container>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={3000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick\n        rtl={false}\n        pauseOnFocusLoss\n        draggable\n        pauseOnHover\n      />\n      <div className=\"card m-3 p-3\">\n        <h2 className=\"text-center mb-3\">\n          <span className=\"text-primary\">Register</span> Account\n        </h2>\n        <Form onSubmit={handleSubmit}>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>First name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"First name\"\n                name=\"firstName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Last name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"Last name\"\n                name=\"lastName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Email address</Form.Label>\n              <Form.Control\n                required\n                type=\"email\"\n                placeholder=\"Enter email\"\n                name=\"email\"\n         