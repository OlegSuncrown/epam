    pauseOnHover\n      />\n      <div className=\"card m-3 p-3\">\n        <h2 className=\"text-center mb-3\">\n          <span className=\"text-primary\">Register</span> Account\n        </h2>\n        <Form onSubmit={handleSubmit}>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>First name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"First name\"\n                name=\"firstName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Last name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"Last name\"\n                name=\"lastName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Email address</Form.Label>\n              <Form.Control\n                required\n                type=\"email\"\n                placeholder=\"Enter email\"\n                name=\"email\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"formBasicPassword\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                minlength=\"6\"\n                placeholder=\"Password\"\n                required\n                name=\"password\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center mt-3\">\n            <Button\n              disabled={loading}\n              to=\"/addGoal\"\n              variant=\"primary\"\n              type=\"submit\"\n              className=\"px-4\"\n            >\n              Register\n            </Button>\n          </Form.Row>\n        </Form>\n      </div>\n    </Container>\n  );\n};\n\nexport default Register;\n","/Users/tetianagurova/workspace/projects/react/epam/src/pages/profile/ProfilePage.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/pages/seasonalGoals/SeasonalGoalsPage.js",["154","155","156"],"import React, { useState, useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { Container, Col, Row } from \"react-bootstrap\";\nimport GoalItem from \"../userGoals/GoalItem\";\n\nconst SeasonalGoals = (props) => {\n  // const goalsList = useContext(GoalsContext);\n\n  return (\n    <Container fluid>\n      <Row>\n        <Col xs={6} md={4}>\n          Title\n        </Col>\n        <Col className=\" col-sm-3\">Start date</Col>\n        <Col className=\"d-none d-sm-block col-sm-3\">End Date</Col>\n        <Col className=\"d-none d-md-block\">Progress</Col>\n      </Row>\n\n      {/* {goalsList.map(item => {\n                 (<GoalItem \n                    title={item.title} \n                    startDate={item.startDate} \n                    endDate={item.endDate} \n                    progress={item.progress} \n                 />) \n              })} */}\n      <GoalItem title=\"1000 push-ups this month\" progress=\"14\" />\n    </Container>\n  );\n};\n\nexport default SeasonalGoals;\n","/Users/tetianagurova/workspace/projects/react/epam/src/pages/userGoals/UserGoalsPage.js",["157","158","159","160","161","162","163"],"/Users/tetianaguro