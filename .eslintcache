draggable: true,\n        progress: undefined,\n      });\n    }\n    setLoading(false);\n  };\n\n  useEffect(() => {\n    if (isAuthenticated) {\n      if (query.get(\"title\")) {\n        history.push(`/dashboard/add-goal${search}`);\n      } else {\n        history.push(\"/dashboard\");\n      }\n    }\n  }, [isAuthenticated]);\n\n  return (\n    <Container>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={3000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick\n        rtl={false}\n        pauseOnFocusLoss\n        draggable\n        pauseOnHover\n      />\n      <div className=\"card m-3 p-3\">\n        <h2 className=\"text-center mb-3\">\n          <span className=\"text-primary\">Log</span> in\n        </h2>\n        <Form onSubmit={handleSubmit} className=\"h-100\">\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Email address</Form.Label>\n              <Form.Control\n                required\n                type=\"email\"\n                placeholder=\"Enter email\"\n                name=\"email\"\n                onChange={onChange}\n              />\n              <Form.Control.Feedback>Looks good!</Form.Control.Feedback>\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"formBasicPassword\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                placeholder=\"Password\"\n                name=\"password\"\n                required\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center mt-3\">\n            <Button\n              to=\"/addGoal\"\n              variant=\"primary\"\n              type=\"submit\"\n              className=\"px-4\"\n              disabled={loading}\n            >\n              Log in\n            </Button>\n          </Form.Row>\n        </Form>\n      </div>\n    </Container>\n  );\n};\n\nexport default LoginPage;\n","/Users/tetianagurova/workspace/projects/react/epam/src/pages/bages/BagesPage.js",[],"/Users/tetianagurova/workspace/projects/react/epam/src/pages/register/RegisterPage.js",["161"],"import React, { useContext, useState, useEffect } from \"react\";\nimport { Form, Button, Col, Container } from \"react-bootstrap\";\nimport { AuthContext } from \"../../context/auth/AuthContext\";\nimport { ToastContainer, toast } from \"react-toastify\";\nconst Register = ({ history }) => {\n  const { registerUser, isAuthenticated } = useContext(AuthContext);\n\n  const [loading, setLoading] = useState(false);\n  const [userRegister, setUserRegister] = useState({\n    firstName: \"\",\n    lastName: \"\",\n    email: \"\",\n    password: \"\",\n  });\n\n  //Redirect user if isAuthenticated\n  useEffect(() => {\n    if (isAuthenticated) {\n      history.push(\"/dashboard\");\n    }\n  }, [isAuthenticated]);\n\n  // Get value from the form\n  const onChange = (e) =>\n    setUserRegister({ ...userRegister, [e.target.name]: e.target.value });\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n\n    try {\n      setLoading(true);\n      return await registerUser(userRegister);\n    } catch (err) {\n      toast.error(err.message, {\n        position: \"top-right\",\n        autoClose: 3000,\n        hideProgressBar: false,\n        closeOnClick: true,\n        pauseOnHover: false,\n        draggable: true,\n        progress: undefined,\n      });\n    }\n\n    setLoading(false);\n  };\n  return (\n    <Container>\n      <ToastContainer\n        position=\"top-right\"\n        autoClose={3000}\n        hideProgressBar={false}\n        newestOnTop={false}\n        closeOnClick\n        rtl={false}\n        pauseOnFocusLoss\n        draggable\n        pauseOnHover\n      />\n      <div className=\"card m-3 p-3\">\n        <h2 className=\"text-center mb-3\">\n          <span className=\"text-primary\">Register</span> Account\n        </h2>\n        <Form onSubmit={handleSubmit}>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>First name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"First name\"\n                name=\"firstName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Last name</Form.Label>\n              <Form.Control\n                required\n                type=\"text\"\n                placeholder=\"Last name\"\n                name=\"lastName\"\n                minlength=\"2\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"validationCustom02\">\n              <Form.Label>Email address</Form.Label>\n              <Form.Control\n                required\n                type=\"email\"\n                placeholder=\"Enter email\"\n                name=\"email\"\n                onChange={onChange}\n              />\n            </Form.Group>\n          </Form.Row>\n          <Form.Row className=\"justify-content-md-center\">\n            <Form.Group as={Col} md=\"4\" controlId=\"formBasicPassword\">\n              <Form.Label>Password</Form.Label>\n              <Form.Control\n                type=\"password\"\n                minlength=\"6\"\n 